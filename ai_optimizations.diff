+// 3. CSV EXPORT WITH LOGO HEADER
+function exportToCSV(data, filename = 'export') {
+    showNotification('Generating CSV export...', 'info');
+    
+    const timestamp = new Date().toISOString().split('T')[0];
+    const fullFilename = `${filename}_${timestamp}.csv`;
+    
+    let csvContent = SYSTEM_LOGO.csvHeader;
+    csvContent += `Report: ${filename}\n`;
+    csvContent += `Generated: ${new Date().toLocaleString()}\n`;
+    csvContent += `User: System Administrator\n\n`;
+    
+    if (Array.isArray(data)) {
+        if (data.length > 0) {
+            const headers = Object.keys(data[0]);
+            csvContent += headers.join(',') + '\n';
+            
+            data.forEach(row => {
+                const values = headers.map(header => {
+                    let value = row[header];
+                    if (typeof value === 'string' && (value.includes(',') || value.includes('"') || value.includes('\n'))) {
+                        value = `"${value.replace(/"/g, '""')}"`;
+                    }
+                    return value;
+                });
+                csvContent += values.join(',') + '\n';
+            });
+        }
+    } else {
+        csvContent += data;
+    }
+    
+    const blob = new Blob([csvContent], { type: 'text/csv;charset=utf-8;' });
+    const url = URL.createObjectURL(blob);
+    const a = document.createElement('a');
+    a.href = url;
+    a.download = fullFilename;
+    document.body.appendChild(a);
+    a.click();
+    document.body.removeChild(a);
+    URL.revokeObjectURL(url);
+    
+    showNotification('CSV exported with company header', 'success');
+}
